&mpivar
px = 2
py = 2
/

&gridvar
nx = 20
ny = 20
nz = 20
dx = 1.0
dy = 1.0
dz = 1.0
periodx = .true.
periody = .true.
periodz = .true.
/

&compvar
tstart = 0.0
tend  = 5.0
dt = 0.2
/

&outvar
output_inteval = 1.0
startframe = 0
/

&iofield
n_vars = 8
io_phi = .true.
io_p   = .true.
io_u   = .true.
io_v   = .true.
io_w   = .true.
io_cx  = .true.
io_cy  = .true.
io_cz  = .true.
output_path = ''
output_name = ''
/

&ns
rho_l = 1000.0
rho_g = 1.0
mu_l  = 1.0e-3
mu_g  = 1.0e-6
iter_tolerance = 1e-3
iter_max = 10
rk_order = 3
/

&ns_bc
phi_bc_types_lo = /(   2,    2,    2 )/
phi_bc_value_lo = /( 0.0,  0.0,  0.0 )/
phi_bc_types_hi = /(   2,    2,    2 )/
phi_bc_value_hi = /( 0.0,  0.0,  0.0 )/

p_bc_types_lo   = /(   2,    2,    2)/
p_bc_value_lo   = /( 0.0,  0.0,  0.0)/
p_bc_types_hi   = /(   2,    2,    2)/
p_bc_value_hi   = /( 0.0,  0.0,  0.0)/

u_bc_types_lo   = /(   1,    1,    1)/
u_bc_value_lo   = /( 0.0,  0.0,  0.0)/
u_bc_types_hi   = /(   1,    1,    1)/
u_bc_value_hi   = /( 0.0,  0.0,  0.0)/

v_bc_types_lo   = /(   1,    1,    1)/
v_bc_value_lo   = /( 0.0,  0.0,  0.0)/
v_bc_types_hi   = /(   1,    1,    1)/
v_bc_value_hi   = /( 0.0,  0.0,  0.0)/

w_bc_types_lo   = /(   1,    1,    1)/
w_bc_value_lo   = /( 0.0,  0.0,  0.0)/
w_bc_types_hi   = /(   1,    1,    1)/
w_bc_value_hi   = /( 0.0,  0.0,  0.0)/
/
